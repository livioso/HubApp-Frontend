{
  "name": "HubApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start -- --reset-cache",
    "packager:kill": "lsof -n -i4TCP:8081 | sed '1 d' | awk '{print $2}' | xargs kill",
    "test": "node node_modules/mocha/bin/mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "lint": "node ./node_modules/eslint/bin/eslint.js --config .eslintrc index.android.js index.ios.js App/ __tests__/; flow || true",
    "lint:fix": "node ./node_modules/eslint/bin/eslint.js --fix --config .eslintrc index.android.js index.ios.js App/"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.0",
    "expect": "^1.20.0",
    "immutable": "^3.8.1",
    "react": "15.0.2",
    "react-native": "^0.26.0",
    "react-native-linear-gradient": "^1.5.6",
    "react-native-vector-icons": "^2.0.3",
    "react-redux": "^4.4.2",
    "redux": "^3.4.0",
    "redux-persist": "^3.2.2",
    "redux-persist-transform-immutable": "^3.1.0",
    "redux-saga": "^0.11.0",
    "redux-thunk": "^2.0.1",
    "remote-redux-devtools": "^0.3.3"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.2",
    "babel-preset-react-native": "^1.7.0",
    "babel-register": "^6.8.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "mocha": "^2.4.5",
    "rnpm-plugin-upgrade": "^0.26.0"
  }
}
